; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Scale Tale"
run/main_scene="res://scenes/config_loader.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
run/max_fps=60
config/icon="res://art/2D/ico.png"

[autoload]

DialogManager="*res://dialogues/dialog_manager.gd"
BeehaveGlobalMetrics="*res://addons/beehave/metrics/beehave_global_metrics.gd"
BeehaveGlobalDebugger="*res://addons/beehave/debug/global_debugger.gd"
CyclopsAutoload="*res://addons/cyclops_level_builder/cyclops_global_scene.tscn"
Persistance="*res://scripts/persistance/persistance.gd"
GrabableDistanceSystem="*res://scripts/singletons/grabable_distance_system.gd"
GlobalSettings="*res://scripts/game_settings/global_settings.gd"
SettingsFile="*res://scripts/game_settings/settings_file.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/resizable=false
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/cyclops_level_builder/plugin.cfg", "res://addons/goshapes/plugin.cfg", "res://addons/terrain_3d/plugin.cfg")

[importer_defaults]

texture={
"compress/channel_pack": 0,
"compress/hdr_compression": 1,
"compress/high_quality": false,
"compress/lossy_quality": 0.7,
"compress/mode": 2,
"compress/normal_map": 0,
"detect_3d/compress_to": 1,
"mipmaps/generate": true,
"mipmaps/limit": -1,
"process/fix_alpha_border": true,
"process/hdr_as_srgb": false,
"process/hdr_clamp_exposure": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"process/size_limit": 0,
"roughness/mode": 8,
"roughness/src_normal": "res://art/2D/textures/freestylized_dot_com/cliff/cliff_rocks_01_normal_dx_1k.png"
}

